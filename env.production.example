# Frontend Production Environment Configuration
REACT_APP_API_URL=https://yourdomain.com
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_BACKEND_URL=https://yourdomain.com
REACT_APP_ADMIN_URL=https://yourdomain.com/admin
REACT_APP_USER_URL=https://yourdomain.com/user

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false

# External Services
REACT_APP_TRIPAY_MERCHANT_CODE=your_merchant_code
REACT_APP_TRIPAY_ENVIRONMENT=production

# Disable source maps in production
GENERATE_SOURCEMAP=false

# Performance optimizations
REACT_APP_ENABLE_ERROR_REPORTING=true 